{
  "manifest_version": 2,
  "name": "Random Dog Picture",
  "version": "1.0",

  "description": "Uses the Dog API to fetch a random picture of a dog whenever you click on the extension",
  
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "browser_action": {
    "default_icon": "icons/icon32.png",
    "default_title": "Randog",
    "default_popup": "popup/randog.html"
  }
}